(define (domain delivery)
    (:requirements :equality :typing)
    ;(:types cell - object locatable - object package - locatable truck - locatable)
    (:predicates
        ;(at ?x1 - locatable ?x2 - cell)
        ;(carrying ?x1 - truck ?x2 - package)
        ;(empty ?x1 - truck)
        ;(adjacent ?x1 - cell ?x2 - cell)

        (at ?loc ?cell)
        (carrying ?t ?p)
        (empty ?t)
        (adjacent ?x1 ?x2)

        (cell_t ?x)
        (locatable_t ?x)
        (package_t ?x)
        (truck_t ?x)
    )

    (:action pick-package
     :parameters (?t ?p ?x)
     :precondition (and (truck_t ?t) (package_t ?p) (cell_t ?x) (at ?p ?x) (at ?t ?x) (empty ?t))
     :effect (and
        (not (at ?p ?x))
        (not (empty ?t))
        (carrying ?t ?p))
    )


    (:action drop-package
     :parameters (?t ?p ?x)
     :precondition (and (truck_t ?t) (package_t ?p) (cell_t ?x) (at ?t ?x) (carrying ?t ?p))
     :effect (and
        (empty ?t)
        (not (carrying ?t ?p))
        (at ?p ?x))
    )


    (:action move
     :parameters (?t ?from ?to)
     :precondition (and (truck_t ?t) (cell_t ?from) (cell_t ?to) (adjacent ?from ?to) (at ?t ?from))
     :effect (and
        (not (at ?t ?from))
        (at ?t ?to))
    )

)
